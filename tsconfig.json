{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es5", "es6", "dom", "dom.iterable"],
    "resolveJsonModule": true
  }
}